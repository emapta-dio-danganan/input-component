{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es2016", 
      "es2017"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": true,
    "module": "esnext",
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "jsx": "react-jsx",
    "outDir": "../../dist/esm",
  
  },


  
}
